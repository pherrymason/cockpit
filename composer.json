{
    "name": "raulferras/cockpit",
    "type": "project",
    "description": "Cockpit Headless CMS",
    "keywords": ["cms", "headles", "api", "cockpit"],
    "homepage": "https://getcockpit.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Heinze",
            "email": "artur@agentejo.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "": ["lib/"],
            "Lime\\": "lib/Lime/",
            "LimeExtra\\": "lib/LimeExtra/"
        },
        "psr-4": {
            "Cockpit\\": "src/Cockpit/",
            "Cockpit\\App\\": "src/Cockpit/App/",
            "Cockpit\\Collections\\": "src/Cockpit/Collections/",
            "Cockpit\\Forms\\": "src/Cockpit/Forms/",
			"Cockpit\\Framework\\": "src/Cockpit/Framework"
        }
    },

    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-fileinfo": "*",

        "phpmailer/phpmailer": "^6.0",
        "claviska/simpleimage": "^3.3",
        "ksubileau/color-thief-php": "^1.3",
        "league/flysystem": "^1.0",
        "firebase/php-jwt": "^5.0",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.7.1",
        "maennchen/zipstream-php": "^0.5.2",
        "php-di/php-di": "^6.0",
        "ramsey/uuid": "^3.9",
        "filp/whoops": "^2.5",
        "cocur/slugify": "^4.0",
        "league/plates": "^3.3",
        "slim/slim": "^4.5",
        "slim/psr7": "^1.1",
        "vlucas/phpdotenv": "^4.1",
        "mezzio/mezzio-authentication": "^1.1",
        "doctrine/dbal": "^2.10",
        "middlewares/whoops": "^2.0",
        "laminas/laminas-diactoros": "^2.3"
    },

    "config": {
        "optimize-autoloader": true
    }
}
